<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junyang.yhack_hireme" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JobSeekerRegisterActivity"
            android:label="@string/title_activity_job_seeker_register" >
        </activity>
        <activity
            android:name=".RecruiterRegisterActivity"
            android:label="@string/title_activity_recruiter_register" >
        </activity>
        <activity
            android:name=".JobSeekerProfileActivity"
            android:label="@string/title_activity_job_seeker_profile" >
        </activity>
    </application>

</manifest>
